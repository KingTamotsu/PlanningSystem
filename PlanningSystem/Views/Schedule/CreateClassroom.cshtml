@model PlanningSystem.Models.Schedule
@{
    ViewBag.Title = "CreateClassroom";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Roostermakerij magic</h2>



@using (Html.BeginForm("CreateScheduleClassroom", "Schedule"))
{
    <div class="form-group">
        @Html.LabelFor(i => i.ClassName.ClassID)

        @Html.DropDownList("StudentClass", (IEnumerable<SelectListItem>)ViewData["ListItemsStudentClasses"])
    </div>
    <button type="submit" class="btn btn-primary">Opslaan</button>
}

